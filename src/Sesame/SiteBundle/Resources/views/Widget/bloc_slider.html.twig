<div class="row">
    <div class="large-12 columns">
        <ul id="slides">
        {%for media in slider %}
            {%if media.getMedia.enabled %}
            <li><img src="{% path media.getMedia, 'slider' %}" alt="{{ media.getMedia.name }}" /></li>
            {% endif %}
        {% endfor %}
        </ul>
    </div>
</div>